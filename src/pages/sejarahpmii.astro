---
import Layout from "../layouts/Layout.astro";
import BaseHead from "../components/BaseHead.astro";
import Timeline from "../components/timeline.astro";
---

<Layout>
    <BaseHead title="Sejarah PMII" description="Halaman sejarah PMII Komfakdish - Website resmi PMII Komfakdish" />
<main class="max-w-2xl mx-auto mb-10 lg:mb-14 px-10 py-10 sm:px-6 lg:px-8">
  <div class="max-w-2xl mx-auto text-center mb-10 lg:mb-14">
    <h1 class="text-2xl font-bold md:text-4xl md:leading-tight dark:text-white">Sejarah PMII</h1>
    <p class="mt-1 text-gray-600 dark:text-neutral-400">PMII KOMFAKDISH</p>
  </div>

  <Timeline />
  </main>
</Layout>